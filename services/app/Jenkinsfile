pipeline{
    agent any

    stages{
        stage('Checkout'){
            steps{
                echo 'Checkinout source code';
                bat 'git clone https://github.com/balujaddu/Ecom-app.git';
            }
        }
        stage('Build'){
            steps{
                echo 'Building java project';
                bat 'mvn clean package';
            }
        }
    }
}
